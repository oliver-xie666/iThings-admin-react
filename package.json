{
  "name": "ithings-admin",
  "version": "5.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "d": "yarn build && bash deploy.sh",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "lint": "max g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss-less",
    "openapi": "max openapi",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "max-serve",
    "start": "cross-env UMI_ENV=dev max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev max dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "pretest": "node ./tests/beforeTest",
    "test": "max test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "max test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --custom-syntax postcss-less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/charts": "^0.9.4",
    "@ant-design/icons": "^4.5.0",
    "@ant-design/plots": "^1.0.9",
    "@antv/data-set": "^0.11.0",
    "@antv/l7": "^2.3.7",
    "@antv/l7-maps": "^2.3.7",
    "@antv/l7-react": "^2.1.9",
    "@antv/s2": "^1.23.0",
    "@antv/s2-react": "^1.21.0",
    "@formily/antd": "^1.3.3",
    "@formily/antd-components": "^1.3.3",
    "@types/pubsub-js": "^1.8.3",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@umijs/route-utils": "^1.0.36",
    "ahooks": "^3.7.0",
    "antd": "^5.1.0",
    "antd-img-crop": "^4.2.4",
    "bizcharts": "^3.5.3-beta.0",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "brace": "^0.11.1",
    "classnames": "^2.2.6",
    "crypto-js": "^4.1.1",
    "echarts": "^5.4.0",
    "echarts-for-react": "^3.0.2",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "ezuikit-js": "^0.6.1",
    "flv.js": "^1.6.2",
    "gg-editor": "^2.0.2",
    "global": "^4.4.0",
    "js-export-excel": "^1.1.4",
    "jsoneditor": "^9.9.2",
    "jsoneditor-react": "^3.1.2",
    "lodash": "^4.17.21",
    "lodash-decorators": "^6.0.0",
    "moment": "^2.29.4",
    "monaco-editor": "0.30.0",
    "monaco-editor-webpack-plugin": "^6.0.0",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "object-sizeof": "^1.6.3",
    "omit.js": "^2.0.2",
    "postcss-px2viewport": "^0.1.3",
    "pubsub-js": "^1.9.4",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-bmapgl": "^0.2.11",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^18.2.0",
    "react-fittext": "^1.0.0",
    "react-helmet-async": "^1.0.4",
    "react-json-editor-ajrm": "^2.5.13",
    "react-monaco-editor": "^0.49.0",
    "react-router": "^4.3.1",
    "styled-components": "^5.3.6",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "swagger-ui-react": "^4.12.0",
    "tree-node-cli": "^1.5.2",
    "umi-presets-pro": "^2.0.0",
    "umi-request": "^1.4.0",
    "umi-serve": "^1.9.10",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@ant-design/plots": "^1.0.9",
    "@ant-design/pro-components": "^2.3.47",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@types/bmapgl": "^0.0.5",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-json-editor-ajrm": "^2.5.3",
    "@umijs/fabric": "^3.0.0",
    "@umijs/max": "^4.0.0",
    "@umijs/openapi": "1.1.14",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.22.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "hooks": "^0.3.2",
    "husky": "4.2.1",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.7.1",
    "puppeteer-core": "^8.0.0",
    "stylelint": "^14.11.0",
    "typescript": "^4.2.2",
    "validate-commit-msg": "^2.14.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
